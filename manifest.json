{
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "run_at": "document_end",
      "matches": [
        "*://torrentz.eu/*",
        "*://torrentz.ch/*",
        "*://torrentz.me/*",
        "*://torrentz.in/*",
        "*://torrentz.com/*"
      ]
    }
  ],
  "description": "Adds next episode links to torrent files on Torrentz.",
  "manifest_version": 3,
  "name": "Next Episode for Torrentz",
  "version": "1.0.2",
  "web_accessible_resources": [
    "icon16.png"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}